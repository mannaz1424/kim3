7.  ㅠㅠㅠ 잘모르겠습니다.

var title = document.querySelectorAll("div h2");
var color = [red, orange, yellow, green, blue, navy, purple];


var title = function() {
	for(i=0; i>title.length; i++) {
		title[i].style.background = color[i];
	}
}

